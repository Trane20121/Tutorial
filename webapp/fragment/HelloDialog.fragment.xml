<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <Dialog
        id="helloDialog"
        title="Hello {/recipient/name}"
    >
        <content>
            <core:Icon id="_IDGenIcon"
                src="sap-icon://hello-world"
                size="500%"
                class="sapUiMediumMargin"
            />
        </content>
        <beginButton>
            <Button
                id="_IDGenButton1"
                text="{i18n>dialogCloseButtonText}"
                press="onCloseDialog"
            />
        </beginButton>
    </Dialog>
</core:FragmentDefinition>
