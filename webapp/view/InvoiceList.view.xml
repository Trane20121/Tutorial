<mvc:View
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
>
    <List id="_IDGenList"
        headerText="{i18n>invoiceListTitle}"
        class="sapUiResponsiveMargine"
        width="auto"
        items="{invoice>/Invoices}"
    >
        <items>
            <ObjectListItem id="_IDGenObjectListItem1"
                title="{invoice>Quantity} x {invoice>ProductName}"
            />
        </items>
    </List>
</mvc:View>
